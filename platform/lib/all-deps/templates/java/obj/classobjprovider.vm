$visibility class $name #class_extends( $superclass ) #class_implements( $interfaces ) {
  
  // Constructors
  
  private ${name}() {}
  
  public static ${name} get() {
    if (PROVIDER == null) {
      PROVIDER = new ${name}();
    }
    return PROVIDER;
  }
  
  
  // Public
  
  /**
   * Create a set of ${class_obj.PrimaryImpl.Name} from an InferenceResultSet.
   * @param resultSet the InferenceResultSet from which to populate the CycModelObject
   * @param column The column to use to populate the ${class_obj.PrimaryImpl.Name}. If column
   * is <code>null</code> <strong>and</strong> the InferenceResultSet has exactly one column, that column
   * will be used.
   * @return
   * @throws ModelAPIException
   * @throws Exception 
   */
  public Set<${class_obj.PrimaryImpl.Name}> createSet(final InferenceResultSet resultSet, final CycVariable column) throws ModelAPIException, Exception {
    final CycVariable bindingVar = (column != null) ? column : findBindingVariable(resultSet);
    final Set<${class_obj.PrimaryImpl.Name}> results = new HashSet<${class_obj.PrimaryImpl.Name}>();
    while (resultSet.next()) {
      final CycObject name = resultSet.getCycObject(bindingVar);
      results.add(new ${class_impl}(name.toString()));
    }
    return results;
  }
  
  public List<${class_obj.PrimaryImpl.Name}> find(final Query query, final CycVariable column, Comparator<${class_obj.PrimaryImpl.Name}> comparator) throws TimeOutException, CycApiException, IOException, ModelAPIException, Exception {
    final List<${class_obj.PrimaryImpl.Name}> results;
    query.performInference();
    final InferenceResultSet rs = query.getResultSet();
    results = new ArrayList(createSet(query.getResultSet(), column));
    Collections.sort(results, comparator);
    return results;
  }
  
  public List<${class_obj.PrimaryImpl.Name}> find(final Query query, final CycVariable var) throws TimeOutException, CycApiException, IOException, ModelAPIException, Exception {
    return find(query, var, getDefaultComparator());
  }
  
  public List<${class_obj.PrimaryImpl.Name}> find(final Query query, Comparator<${class_obj.PrimaryImpl.Name}> comparator) throws TimeOutException, CycApiException, IOException, ModelAPIException, Exception {
    return find(query, null, comparator);
  }
  
  public List<${class_obj.PrimaryImpl.Name}> find(final Query query) throws TimeOutException, CycApiException, IOException, ModelAPIException, Exception {
    return find(query, getDefaultComparator());
  }
  
  
  // Protected
  
  protected Comparator<${class_obj.PrimaryImpl.Name}> getDefaultComparator() {
    return DEFAULT_COMPARATOR;
  }
  
  
  // Internal
  
  private static ${name} PROVIDER;
  private static Comparator<${class_obj.PrimaryImpl.Name}> DEFAULT_COMPARATOR = new Comparator<${class_obj.PrimaryImpl.Name}>() {
    @Override
    public int compare(${class_obj.PrimaryImpl.Name} o1, ${class_obj.PrimaryImpl.Name} o2) {
      return o1.getInstance().toString().compareTo(o2.getInstance().toString());
    }
  };
}
