@package org.opencyc.chat;

/**
 * Contains the attributes and behavior of a chat dialog Finite
 * State Machine Arc.<p>
 *
 * @@version $Id: DialogFsmArc.java,v 1.1 2002/02/11 09:21:38 stephenreed Exp $
 * @@author Stephen L. Reed
 *
 * <p>Copyright 2001 Cycorp, Inc., license is open source GNU LGPL.
 * <p><a href="http://www.opencyc.org/license.txt">the license</a>
 * <p><a href="http://www.opencyc.org">www.opencyc.org</a>
 * <p><a href="http://www.sourceforge.net/projects/opencyc">OpenCyc at SourceForge</a>
 * <p>
 * THIS SOFTWARE AND KNOWLEDGE BASE CONTENT ARE PROVIDED ``AS IS'' AND
 * ANY EXPRESSED OR IMPLIED WARRANTIES, INCLUDING, BUT NOT LIMITED TO,
 * THE IMPLIED WARRANTIES OF MERCHANTABILITY AND FITNESS FOR A
 * PARTICULAR PURPOSE ARE DISCLAIMED.  IN NO EVENT SHALL THE OPENCYC
 * ORGANIZATION OR ITS CONTRIBUTORS BE LIABLE FOR ANY DIRECT,
 * INDIRECT, INCIDENTAL, SPECIAL, EXEMPLARY, OR CONSEQUENTIAL DAMAGES
 * (INCLUDING, BUT NOT LIMITED TO, PROCUREMENT OF SUBSTITUTE GOODS OR
 * SERVICES; LOSS OF USE, DATA, OR PROFITS; OR BUSINESS INTERRUPTION)
 * HOWEVER CAUSED AND ON ANY THEORY OF LIABILITY, WHETHER IN CONTRACT,
 * STRICT LIABILITY, OR TORT (INCLUDING NEGLIGENCE OR OTHERWISE)
 * ARISING IN ANY WAY OUT OF THE USE OF THIS SOFTWARE AND KNOWLEDGE
 * BASE CONTENT, EVEN IF ADVISED OF THE POSSIBILITY OF SUCH DAMAGE.
 */

public class DialogFsmArc {

    /**
     * finite state machine event
     */
    protected DialogFsmEvent dialogFsmEvent;

    /**
     * finite state machine state transition to node
     */
    protected DialogFsmNode dialogFsmTransitionToNode;

    /**
     * finite state machine state action
     */
    protected DialogFsmAction dialogFsmAction;

    /**
     * Constructs a new DialogFsmArc object, given the current node, event,
     * transition-to node and the action to take.  Hooks the new arc into
     * the current node.
     *
     * @@param dialogFsmCurrentNode the currentNode
     * @@param dialogFsmEvent the transition trigger event
     * @@param dialogFsmTransitionToNode the transition-to node
     * @@param dialogFsmAction the action to take
     */
    public DialogFsmArc (DialogFsmNode dialogFsmCurrentNode,
                         DialogFsmEvent dialogFsmEvent,
                         DialogFsmNode dialogFsmTransitionToNode,
                         DialogFsmAction dialogFsmAction) {
        this.dialogFsmEvent = dialogFsmEvent;
        this.dialogFsmTransitionToNode = dialogFsmTransitionToNode;
        this.dialogFsmAction = dialogFsmAction;
        dialogFsmCurrentNode.addDialogFsmArc(this);
    }

    /**
     * Returns the finite state machine state event.
     *
     * @@return the finite state machine state event
     */
    public DialogFsmEvent getDialogFsmEvent () {
        return dialogFsmEvent;
    }

    /**
     * Returns the finite state machine state transition to node.
     *
     * @@return the finite state machine state transition to node
     */
    public DialogFsmNode getDialogFsmTransitionToNode () {
        return dialogFsmTransitionToNode;
    }

    /**
     * Returns the finite state machine state action.
     *
     * @@return the finite state machine state action
     */
    public DialogFsmAction getDialogFsmAction () {
        return dialogFsmAction;
    }
}@


